{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start":
      "webpack-dev-server --hot --inline --config webpack.dev.js --mode development",
    "build":
      "NODE_ENV=production webpack --config webpack.prod.js --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard src/**/*",
    "dist":
      "npm run lint && npm run build:client && npm run copy && npm run sw",
    "copy": "cp index.html dist/",
    "sw": "sw-precache --root=dist"
  },
  "author": "maxmechanic",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "autoprefixer": "^8.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.11",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.1",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {}
}
